@import '../../../styles/settings/mixin.styl'

.l-services
  display: flex
  justify-content: space-between
  margin 0 -20px
  &__item
    padding 0 20px
  &__img
    width: 110px
    height: 95px
    display: block
    object-fit contain
    object-position center
    margin: 0 auto
  &__info
    padding: 10px 20px
    border: 1px solid #1f2532
    margin-top: 15px
    font-size 1.4rem
    text-transform: uppercase
    osans(600)

